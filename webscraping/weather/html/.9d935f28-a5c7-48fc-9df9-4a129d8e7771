        function formRedirect(url) {
            $('.tatsu-form-submit').on('submit', () => {
                let newATag = document.createElement('a');
                newATag.href = url.toString();
                setTimeout(() => {
                    newATag.click();
                }, 2000)
            })
        }
        function changeLinks() {
            let links = $("a").filter(function () { return this.attributes['href']; });
            let hasQueryParams = window.location.href.includes("?");
            let formRedirectUrl = 'https://ucyfx.com/click';
            let newFormRedirectUrl = new URL(formRedirectUrl);
            if (hasQueryParams) {
                let queryParams = {};
                let currentUrl = new URL(window.location.href);
                let urlParams = new URLSearchParams(currentUrl.search);
                for (let val of urlParams.keys()) {
                    queryParams[val] = urlParams.get(val);
                }
                links.each(function (index) {
                    let cur = $(this);
                    let href = cur.attr('href');
                    try {
                        let newUrl = new URL(href);
                        Object.keys(queryParams).forEach(queryParam => {
                            if (queryParam === 'gclid') {
                                newUrl.searchParams.set('sub1', queryParams[queryParam])
                                newFormRedirectUrl.searchParams.set('sub1', queryParams[queryParam])
                                newUrl.searchParams.delete('gclid')
                            } else if (queryParam === 'fbclid') {
                                newUrl.searchParams.set('sub2', queryParams[queryParam])
                                newFormRedirectUrl.searchParams.set('sub2', queryParams[queryParam])
                                newUrl.searchParams.delete('fbclid')
                            } else if (queryParam === 's5') {
                                newUrl.searchParams.set('s5', queryParams[queryParam])
                                newFormRedirectUrl.searchParams.set('s5', queryParams[queryParam])
                            } else if (queryParam === 'sub5') {
                                newUrl.searchParams.set('sub5', queryParams[queryParam])
                                newFormRedirectUrl.searchParams.set('sub5', queryParams[queryParam])
                            } else if (queryParam === 'sub2') {
                                newUrl.searchParams.set('sub2', queryParams[queryParam])
                                newFormRedirectUrl.searchParams.set('sub2', queryParams[queryParam])
                            } else if (queryParam === 'link_id') {
                                newUrl.searchParams.set('link_id', queryParams[queryParam])
                                newFormRedirectUrl.searchParams.set('link_id', queryParams[queryParam])
                            }
                        })
                        cur.attr('href', newUrl.href);
                        cur.data('tracking-href', href)
                    } catch (e) {
                        console.error("Unable to append query parameters. Please add a valid URL")
                    }
                })
            }
            formRedirect(newFormRedirectUrl)
        }
        changeLinks();
    })</script><!--/sp-tracking-page-bodyClose--><script type="text/javascript" src="../scripts/sp-events.js"></script><script type="text/javascript" src="../scripts/tatsu.min.js"></script><script async type="text/javascript" src="../scripts/analytics.min.js"></script></html>